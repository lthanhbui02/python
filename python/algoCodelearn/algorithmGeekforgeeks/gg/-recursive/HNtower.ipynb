   "source": [
    "def Hanoi(n, start, middle, end):\n",
    "    if n == 1:\n",
    "        print(\"Move\", n, \"from\", start, \"to\", end)\n",
    "        return()\n",
    "    Hanoi(n - 1, start, end, middle)\n",
    "    print(\"Move\", n, \"from\", start, \"to\", end)\n",
    "    Hanoi(n - 1, middle, start, end)\n",
    "Hanoi(3, \"start\", \"middle\", \"end\")"
   ]
  }
 ],
